{
    "name": "guava/filament-drafts",
    "description": "A filament plugin that adds the ability to manage drafts and revisions of your models.",
    "type": "library",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Lukas Frey",
            "email": "lukas.frey@guava.cz"
        }
    ],
    "require": {
        "filament/filament": "^3.0-stable",
        "oddvalue/laravel-drafts": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Guava\\FilamentDrafts\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Guava\\FilamentDrafts\\FilamentDraftsServiceProvider"
            ]
        }
    }
}
